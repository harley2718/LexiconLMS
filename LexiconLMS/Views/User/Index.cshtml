@model IEnumerable<LexiconLMS.Models.ViewModels.UserViewModel>

@{
    ViewBag.Title = "Användare";
}

<h2>Användare</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserFName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserLName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserEmail)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserPhoneNumber)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserFName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserLName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserEmail)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserPhoneNumber)
        </td>
  
        <td>
            @Html.ActionLink("Redigera", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Detaljer", "Details", new { id=item.Id }) |
            @Html.ActionLink("Radera", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
